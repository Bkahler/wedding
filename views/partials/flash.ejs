  <div id="flash" class="container">
    <% if(error && error.length > 0 ) { %>
      <div id="danger" class="alert alert-danger other"><%= error %></div>
    <% } else if (success && success.length > 0 ){ %>
      <div id="success" class="alert alert-success other"><%= success %></div> 
    <% } %>
  </div>